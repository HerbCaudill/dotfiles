{
  "permissions": {
    "allow": [
      "Skill(superpowers:*)",
      "Bash(npm:*)",
      "Bash(pnpm:*)",
      "Bash(yarn:*)",
      "Bash(npx:*)",
      "Bash(node:*)",
      "Bash(git:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(rm:*)",
      "Bash(mv:*)",
      "Bash(cp:*)",
      "Bash(cat:*)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(wc:*)",
      "Bash(which:*)",
      "Bash(echo:*)",
      "Bash(pwd)",
      "Read(*)",
      "Write(*)",
      "Edit(*)",
      "MultiEdit(*)",
      "Git(*)",
      "Curl(*)",
      "HTTP(*)",
      "NPM(*)",
      "Vitest(*)",
      "TodoRead(*)",
      "TodoWrite(*)",
      "mcp__context7:*",
      "Bash(tree:*)",
      "Bash(claude mcp:*)",
      "Bash(pnpm install)",
      "Bash(pnpm add:*)",
      "Bash(pnpm run build:*)",
      "Bash(pnpm remove:*)",
      "Skill(creating-web-apps)",
      "Bash(curl:*)"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "dontAsk"
  },
  "model": "opus",
  "enableAllProjectMcpServers": false,
  "enabledMcpjsonServers": [],
  "hooks": {
    "PreCompact": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bd prime"
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bd prime"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Skill",
        "hooks": [
          {
            "type": "command",
            "command": "printf '\\033]0;Claude: /%s\\007' \"$TOOL_INPUT_skill\""
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "node $HOME/.claude/statusline.js"
  },
  "enabledPlugins": {
    "beads@beads-marketplace": true,
    "context7@claude-plugins-official": true,
    "playwright@claude-plugins-official": true,
    "typescript-lsp@claude-plugins-official": true,
    "vercel@claude-plugins-official": true,
    "serena@claude-plugins-official": true
  },
  "autoUpdatesChannel": "latest"
}
